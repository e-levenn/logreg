<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://schemas.android.com/tools http://schemas.android.com/apk/res/android ">

    <uses-feature
        android:name="android.hardware.telephony"
        android:required="false" />

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:label="@string/app_name"
        android:theme="@style/Theme.Loginreg">
        <activity
            android:name=".CardActivity"
            android:exported="false"
            android:label="@string/title_activity_card"
            android:theme="@style/Theme.Loginreg" />
        <activity
            android:name=".CustomActivity"
            android:exported="false"
            android:label="@string/title_activity_custom" />
        <activity
            android:name=".SimplelistActivity"
            android:exported="false"
            android:label="@string/title_activity_simplelist" />
        <activity
            android:name=".simplelist"
            android:exported="false"
            android:label="@string/title_activity_simplelist" />
        <activity
            android:name=".CalcActivity2"
            android:exported="false"
            android:label="@string/title_activity_calc2"
            android:theme="@style/Theme.Loginreg" />
        <activity
            android:name=".calcactivity"
            android:exported="false"
            android:label="@string/title_activity_calcactivity"
            android:theme="@style/Theme.Loginreg" />
        <activity
            android:name=".ui.theme.calcactivity"
            android:exported="false"
            android:label="@string/title_activity_calcactivity" />
        <activity
            android:name=".ui.theme.CalcActivity"
            android:exported="false"
            android:label="@string/title_activity_calc" />
        <activity
            android:name=".IntentActivity"
            android:exported="false"
            android:label="@string/title_activity_intent" />
        <activity
            android:name=".RegisterActivity"
            android:exported="false"
            android:label="@string/title_activity_register" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>